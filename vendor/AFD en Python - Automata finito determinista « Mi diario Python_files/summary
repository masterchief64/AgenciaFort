sta se pueden utilizar los métodos siguientes:\n\n\u003E\u003E\u003Enum = [6, 2, 5, 8, 11, 3]\n\u003E\u003E\u003Eprint "},"link":[{"rel":"replies","type":"application/atom+xml","href":"http:\/\/www.pythondiario.com\/feeds\/3006321816596435847\/comments\/default","title":"Enviar comentarios"},{"rel":"replies","type":"text/html","href":"http:\/\/www.pythondiario.com\/2014\/09\/modulo-bisect-en-python-listas-ordenadas.html#comment-form","title":"0 comentarios"},{"rel":"edit","type":"application/atom+xml","href":"http:\/\/www.blogger.com\/feeds\/9008053624313923355\/posts\/default\/3006321816596435847"},{"rel":"self","type":"application/atom+xml","href":"http:\/\/www.blogger.com\/feeds\/9008053624313923355\/posts\/default\/3006321816596435847"},{"rel":"alternate","type":"text/html","href":"http:\/\/www.pythondiario.com\/2014\/09\/modulo-bisect-en-python-listas-ordenadas.html","title":"Modulo bisect en python (listas ordenadas)"}],"author":[{"name":{"$t":"Diego Caraballo"},"uri":{"$t":"https:\/\/plus.google.com\/100420156123567362280"},"email":{"$t":"noreply@blogger.com"},"gd$image":{"rel":"http://schemas.google.com/g/2005#thumbnail","width":"32","height":"32","src":"\/\/lh5.googleusercontent.com\/-p9qwZ7g2NaU\/AAAAAAAAAAI\/AAAAAAAAEH0\/BXcvcTOCPsg\/s512-c\/photo.jpg"}}],"media$thumbnail":{"xmlns$media":"http://search.yahoo.com/mrss/","url":"http:\/\/1.bp.blogspot.com\/-5NsIMwEewHY\/VEU1gZ4m0FI\/AAAAAAAAEBk\/BEYQ4GRYljw\/s72-c\/modulo%2Bbisect.jpg","height":"72","width":"72"},"thr$total":{"$t":"0"}},{"id":{"$t":"tag:blogger.com,1999:blog-9008053624313923355.post-5344030536093293508"},"published":{"$t":"2014-09-24T09:47:00.000-07:00"},"updated":{"$t":"2014-09-25T05:41:18.618-07:00"},"category":[{"scheme":"http://www.blogger.com/atom/ns#","term":"ejemplos listas"},{"scheme":"http://www.blogger.com/atom/ns#","term":"ejercicios"},{"scheme":"http://www.blogger.com/atom/ns#","term":"ejercicios de programacion"},{"scheme":"http://www.blogger.com/atom/ns#","term":"listas"},{"sch